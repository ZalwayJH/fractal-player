    "autoprefixer": "^10.4.16",
    "electron": "^25.6.0",
    "electron-builder": "^24.6.3",
    "electron-vite": "^1.0.27",
    "postcss": "^8.4.32",
    "prettier": "^3.0.2",
    "react": "^18.2.0",
